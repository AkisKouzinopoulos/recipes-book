{
  "name": "recipes-book",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.7",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.2",
    "@mui/x-data-grid": "^5.12.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.38",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typeface-montserrat": "^1.1.13",
    "typescript": "^4.6.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --fix --ext .js,.jsx src/",
    "lint:check": "eslint --ext .js,.jsx src/",
    "prettier": "prettier --write 'src/**/*.{js,jsx}'",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx}'",
    "prepare": "husky install",
    "fake-server": "json-server --id id --watch fake-data/db.json --routes fake-data/routes.json --port 3001",
    "dev-server": "concurrently \"yarn start\" \"yarn fake-server\"",
    "dev-storybook": "concurrently \"yarn start\" \"yarn fake-server\" \"yarn storybook\""
  },
  "lint-staged": {
    "*.{js,jsx,js,jsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.jsx",
      "!**/node_modules/**",
      "!build/**",
      "!src/**/*.stories.jsx",
      "!src/**/*.jsx.snap",
      "!src/App.jsx",
      "!src/index.jsx",
      "!src/clients/models/**/*.jsx",
      "!src/pages/Paths.jsx"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.17.12",
    "@babel/runtime": "^7.17.9",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.0",
    "@storybook/addon-links": "^6.5.3",
    "@storybook/builder-webpack5": "^6.5.0",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/node-logger": "^6.5.4",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@types/react-test-renderer": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "2.6.2",
    "react-test-renderer": "^17.0.2",
    "stylelint-config-prettier": "^9.0.3",
    "webpack": "^5.73.0"
  }
}
